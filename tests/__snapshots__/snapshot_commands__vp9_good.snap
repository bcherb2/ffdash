---
- ffmpeg -i /tmp/input.mp4 -progress - -nostats -c:v libvpx-vp9 -b:v 0 -crf 31 -quality good -cpu-used 2 -profile:v 0 -pix_fmt yuv420p -row-mt 1 -tile-columns 2 -tile-rows 0 -g 240 -lag-in-frames 25 -auto-alt-ref 1 -aq-mode 1 -arnr-maxframes 7 -arnr-strength 3 -enable-tpl 1 -c:a libopus -b:a 128k -vbr:a on -compression_level:a 10 -y /tmp/output.webm
